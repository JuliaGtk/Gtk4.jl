  const FLOAT = 1
  const INT = 2
  const UINT = 3
  const BOOL = 4
  const VEC2 = 5
  const VEC3 = 6
  const VEC4 = 7
  end)))
  $(Expr(:toplevel, :(baremodule RenderNodeType
  const NOT_A_RENDER_NODE = 0
  const CONTAINER_NODE = 1
  const CAIRO_NODE = 2
