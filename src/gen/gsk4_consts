  const BLUR_NODE = 23
  const DEBUG_NODE = 24
  const GL_SHADER_NODE = 25
  end)))
  $(Expr(:toplevel, :(baremodule ScalingFilter
  const LINEAR = 0
  const NEAREST = 1
  const TRILINEAR = 2
  end)))
  $(Expr(:toplevel, :(baremodule SerializationError
