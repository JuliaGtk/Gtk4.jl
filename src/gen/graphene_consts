quote
$(Expr(:toplevel, quote
    $(Expr(:toplevel, :(module Constants
  PI = 3.141593
  PI_2 = 1.570796
  VEC2_LEN = 2
  VEC3_LEN = 3
  VEC4_LEN = 4
  $(Expr(:toplevel, :(baremodule EulerOrder
  const DEFAULT = -1
  const XYZ = 0
  const YZX = 1
  const ZXY = 2
  const XZY = 3
  const YXZ = 4
  const ZYX = 5
  const SXYZ = 6
  const SXYX = 7
  const SXZY = 8
