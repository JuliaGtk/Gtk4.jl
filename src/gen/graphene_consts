  const RZYZ = 29
  end)))
  $(Expr(:toplevel, :(baremodule RayIntersectionKind
  const NONE = 0
  const ENTER = 1
  const LEAVE = 2
  end)))
  export EulerOrder, RayIntersectionKind
  end)))
end))
